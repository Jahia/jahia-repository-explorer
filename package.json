{
  "name": "@jahia/jahia-repository-explorer",
  "version": "0.0.1",
  "husky": {
    "hooks": {
      "pre-push": "yarn lint:fix"
    }
  },
  "scripts": {
    "build": "yarn lint:fix && yarn webpack",
    "build:nolint": "yarn webpack",
    "dev": "yarn webpack --watch",
    "webpack": "node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js",
    "build:analyze": "yarn build --analyze",
    "build:production": "yarn build --mode=production",
    "build:production-analyze": "yarn build --mode=production --analyze",
    "clean": "rimraf *.log src/main/resources/javascript/apps",
    "clean:all": "yarn clean && rimraf node_modules node",
    "lint": "eslint --ext js,jsx .",
    "lint:fix": "eslint --ext js,jsx --fix ."
  },
  "main": "index.js",
  "license": "MIT",
  "dx-extends": {
    "@jahia/jahia-ui-root": "0.0.1"
  },
  "jahia": {
    "remotes": {
      "jahia": "javascript/apps/remoteEntry.js"
    }
  },
  "resolutions": {
    "minimist": "1.2.6"
  },
  "dependencies": {
    "@jahia/moonstone": "^2.1.0",
    "@jahia/ui-extender": "^1.0.3",
    "graphql-tag": "^2.12.6",
    "prop-types": "^15.8.1",
    "react": "^16.13.1",
    "react-apollo": "^3.1.4",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.15.3",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.4",
    "@jahia/eslint-config": "^1.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.1",
    "eslint": "^6.7.2",
    "eslint-loader": "3.0.3",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^6.2.0",
    "husky": "^3.0.9",
    "node-sass": "^7.0.1",
    "path": "^0.12.7",
    "rimraf": "^3.0.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  }
}
